  "scripts": {
    "test": "npm run mocha && npm run eslint",
    "fix": "npm run eslint-fix",
    "eslint": "eslint . --ignore-path .gitignore --ext .js,.jsx",
    "eslint-fix": "( git diff --staged --name-only | grep \".jsx\\?$\" | xargs eslint --fix ) && ( git diff --staged --name-only | grep \".jsx\\?$\" | xargs git add )",
    "mocha": "mocha $(find . -name 'test.js' ! -ipath '*node_modules*')"
  },
  "pre-commit": [
    "fix",
    "test"
  ],
  "dependencies": {
    "debug": "^2.2.0",
    "pluralize": "^1.2.1",
