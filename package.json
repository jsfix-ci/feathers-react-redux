    "redux-actions": "^0.9.1",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "eslint-config-xo": "^0.9.1",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "^2.4.5",
